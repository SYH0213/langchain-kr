    "def add_numbers(a, b):\n",
    "    result = a + b\n",
    "    return result\n",
    "\n",
    "def main():\n",
    "    numbers = [1, 2, 3, 4, 5]\n",
    "    total = 0\n",
    "\n",
    "    for num in numbers:\n",
    "        total = add_numbers(total, num)\n",
    "        print(f\"현재까지 합: {total}\")\n",
    "\n",
    "    print(\"최종 합:\", total)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
  }
